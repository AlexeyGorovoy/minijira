<?xml version="1.0" encoding="UTF-8"?>
<!-- This file is not required if you don't need any extra configuration. -->
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <!-- This descriptor activates the JSF 2.0 Servlet -->

    <!-- Write your navigation rules here. You are encouraged to use CDI for
       creating @Named managed beans. -->

    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <managed-bean>
        <managed-bean-name>sample</managed-bean-name>
        <managed-bean-class>com.mycompany.firstapp.web.SampleBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
	
	<managed-bean>
        <managed-bean-name>project</managed-bean-name>
        <managed-bean-class>com.mycompany.firstapp.web.ProjectBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
		<managed-property>
            <property-name>sample</property-name>
            <value>#{sample}</value>
        </managed-property>
    </managed-bean>
	
	
	<managed-bean>
        <managed-bean-name>personB</managed-bean-name>
        <managed-bean-class>com.mycompany.firstapp.web.PersonBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
		<managed-property>
            <property-name>sample</property-name>
            <value>#{sample}</value>
        </managed-property>
    </managed-bean>

	
	<managed-bean>
        <managed-bean-name>skillB</managed-bean-name>
        <managed-bean-class>com.mycompany.firstapp.web.SkillBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
		<managed-property>
            <property-name>sample</property-name>
            <value>#{sample}</value>
        </managed-property>
    </managed-bean>
	
	<managed-bean>
        <managed-bean-name>customer</managed-bean-name>
        <managed-bean-class>com.mycompany.firstapp.web.CustomerBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
		<managed-property>
            <property-name>sample</property-name>
            <value>#{sample}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>adder</managed-bean-name>
        <managed-bean-class>com.mycompany.firstapp.web.AdderBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>


</faces-config>
